{
    "version": 2,
    "builds": [
      {
        "src": "app/**",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/app/$1"
      }
    ],
    "engines": {
      "node": "18.x"
    }
  }