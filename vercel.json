{
    "version": 2,
    "builds": [
      {
        "src": "app/api/download/route.js",
        "use": "@vercel/node"
      },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/download",
        "dest": "/app/api/download/route.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }